#/home/asuka/.screenlayout/screens.sh &&
xrdb -merge ~/.Xresources &
dunst &
/home/asuka/.config/polybar/launch.sh &
feh --bg-fill /home/asuka/.wallpapers/invert_bottom.jpg --bg-fill /home/asuka/.wallpapers/invert_top.jpg --bg-fill /home/asuka/.wallpapers/invert_top.jpg --bg-fill /home/asuka/.wallpapers/invert_top.jpg &
picom -b --config /home/asuka/.config/picom.conf
